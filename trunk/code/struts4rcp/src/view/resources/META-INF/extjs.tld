			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description><![CDATA[tag id]]></description>
		</attribute>
		<attribute>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description><![CDATA[]]></description>
		</attribute>
